{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": false
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": false,
    "es6": true
  },
  "rules": {
    "space-before-function-paren": "off",
    "import/prefer-default-export": "off",
    "linebreak-style": "off",
    "global-require": "off",
    "semi": "off",
    "arrow-body-style": "off",
    "no-multiple-empty-lines": ["warn", {"max": 1}]
  }
}
